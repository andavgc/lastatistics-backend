{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "install": {
    "command": "pip install -r requirements.txt"
  },
  "build": {
    "command": "uvicorn main:app --host 0.0.0.0 --port $PORT",
    "use": "@vercel/python"
  }
}
